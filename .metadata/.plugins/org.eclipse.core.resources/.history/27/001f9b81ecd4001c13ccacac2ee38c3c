package test;

public class test5 {

	public static void main(String[] args) {
		
		int scores[] =   {98, 70, 45, 60, 83};
		
		int rank[] = new int [5];
		
		for (int i = 0; i < scores.length; i++) {
			rank[i] = 1;
			for (int j = 0; j < rank.length; j++) {
				if (scores[i] == scores[j])
				
					rank [i] = 1;
				
			}
		}
		for (int i = 0; i < scores.length; i++) {
			System.out.println("점수"+ scores[i] + "\t" + "등수" +rank[i]);
			
		}


	}

}
