package test;

public class test3 {

	public static void main(String[] args) {

		String[] oArray = {"aaa","bbb","ccc","ddd","eee"};
		String[] nArray = new String[5];
		
		System.arraycopy(oArray, 0, nArray, 0, oArray.length);
		
		for (int i = 0; i < nArray.length; i++) {
			System.out.print(nArray[i]+ "\t" );
	}

	}}
