package test;

public class test2 {

	public static void main(String[] args) {

		int[] scores =   {98, 70, 45, 60, 83};
		
        int sum = 0;
                
		
		for (int score : scores) {
			sum += score;
		}
        System.out.println("총합 =" + sum);
		
		double avg = (double) sum /scores.length;
		System.out.println("평균 =" + avg);
		
		System.out.println();
	}

}
