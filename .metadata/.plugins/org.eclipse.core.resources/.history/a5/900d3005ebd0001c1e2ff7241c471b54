package p240;

import java.lang.reflect.Method;

public class Class {
	int field1;
	void method1() {}
	
	static int field1;
	static void method1() {}
	
	static {
		field1 = 10;
		method1();
		field2 = 1;
		Method2();
	}
	
	static void Method3() {
		this.field1 = 10;
		this.method1();
		field2 = 10;
		method2();
	}
}
