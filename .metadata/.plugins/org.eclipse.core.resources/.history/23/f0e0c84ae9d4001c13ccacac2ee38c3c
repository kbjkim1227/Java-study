package chap4;

public class SwitchExample {

	public static void main(String[] args) {
		int num = 100; char jumsu;
		
		switch(num/10) {
		case 1 :
			jumsu = 'A';
			break;
		
		case 2 :
			jumsu = 'B';
			break;
			
		case 3 :
			jumsu = 'C';
			break;
			
		case 4 :
			jumsu = 'D';
			break;
			
		case 5 :
			jumsu = 'F';
			
			System.out.println("학점은" + jumsu +"임");
			
			
		}

	}

}
